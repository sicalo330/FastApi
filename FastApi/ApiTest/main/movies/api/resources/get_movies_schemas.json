{
    "$schema": "https://json-schema.org/draft/2019-09/schema&quot",
    "$id": "http://example.com/example.json&quot",
    "type": "array",
    "default": [],
    "title": "Root Schema",
    "items": {
        "type": "object",
        "default": {},
        "title": "A Schema",
        "required": [
            "id",
            "title",
            "overview",
            "year",
            "rating",
            "category"
        ],
        "properties": {
            "id": {
                "type": "integer",
                "default": 0,
                "title": "The id Schema",
                "examples": [
                    1
                ]
            },
            "title": {
                "type": "string",
                "default": "",
                "title": "The title Schema",
                "examples": [
                    "Mi Pelicula"
                ]
            },
            "overview": {
                "type": "string",
                "default": "",
                "title": "The overview Schema",
                "examples": [
                    "Descripcion de la pelicula"
                ]
            },
            "year": {
                "type": "integer",
                "default": 0,
                "title": "The year Schema",
                "examples": [
                    2022
                ]
            },
            "rating": {
                "type": "number",
                "default": 0.0,
                "title": "The rating Schema",
                "examples": [
                    9.9
                ]
            },
            "category": {
                "type": "string",
                "default": "",
                "title": "The category Schema",
                "examples": [
                    "Acción"
                ]
            }
        },
        "examples": [{
            "id": 1,
            "title": "Mi Pelicula",
            "overview": "Descripcion de la pelicula",
            "year": 2022,
            "rating": 9.9,
            "category": "Acción"
        }]
    },
